{
  "name": "dark-lang",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "next dev --turbopack --experimental-https",
    "tg": "ngrok http --domain tapir-valid-pelican.ngrok-free.app 3000",
    "build": "next build",
    "start": "next start",
    "lint": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:e2e": "playwright test",
    "e2e": "pnpm test:e2e",
    "e2e:ui": "pnpm test:e2e:ui",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "ai-translate": "node --env-file=.env.tool updateLanguage.mjs",
    "a": "lingui extract",
    "lang": "lingui extract && npm run ai-translate && lingui compile",
    "lang-clean": "lingui extract --clean && npm run lang",
    "stripe-hook": "stripe listen --forward-to localhost:3000/api/webhooks/stripe"
  },
